  "devDependencies": {
    "eslint": "^8.56.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
