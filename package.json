{
  "name": "cv-auth-secure",
  "version": "1.0.0",
  "description": "Application sécurisée pour CV avec Google OAuth et 2FA",
  "main": "serveur.js",
  "type": "module",
  "scripts": {
    "start": "node serveur.js",
    "dev": "nodemon serveur.js",
    "build": "echo 'No build step required'",
    "lint": "eslint . --ext .js,.mjs",
    "format": "prettier --write \"**/*.{js,mjs,json,css,html}\"",
